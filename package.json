{
  "name": "@hahow/hahow-design",
  "version": "0.8.0",
  "description": "hahow-design React component",
  "main": "lib/index.js",
  "module": "es/index.js",
  "files": [
    "css",
    "es",
    "lib",
    "umd"
  ],
  "scripts": {
    "build-storybook": "build-storybook -s public",
    "build": "nwb build-react-component --no-demo",
    "clean": "nwb clean-module && nwb clean-demo",
    "lint": "eslint src",
    "prepublishOnly": "npm run build",
    "snapshot": "build-storybook && percy-storybook",
    "start": "npm run storybook",
    "storybook": "start-storybook -p 6006 -s public",
    "test": ""
  },
  "dependencies": {
    "antd": "^3.26.0",
    "artplayer-react": "^3.3.2",
    "format-duration": "^1.3.1",
    "lodash": "^4.17.15",
    "pluralize": "^8.0.0",
    "prop-types": "^15.7.2",
    "react-collapsed": "^2.2.3",
    "react-highlight-words": "^0.16.0",
    "react-style-proptype": "^3.2.2",
    "styled-components": "^4.4.1",
    "styled-system": "^5.1.4"
  },
  "peerDependencies": {
    "react": "16.x"
  },
  "devDependencies": {
    "@babel/core": "^7.7.4",
    "@percy/storybook": "^3.2.0",
    "@semantic-release/git": "^7.0.18",
    "@storybook/addon-actions": "^5.2.8",
    "@storybook/addon-knobs": "^5.2.8",
    "@storybook/addon-links": "^5.2.8",
    "@storybook/addon-viewport": "^5.2.8",
    "@storybook/addons": "^5.2.8",
    "@storybook/react": "^5.2.8",
    "babel-loader": "^8.0.6",
    "babel-plugin-import": "^1.13.0",
    "babel-plugin-inline-react-svg": "^0.5.4",
    "eslint": "^6.1.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-react-hooks": "^1.7.0",
    "nwb": "0.23.x",
    "react": "^16.12.0",
    "react-dom": "^16.12.0"
  },
  "author": "",
  "homepage": "",
  "license": "MIT",
  "repository": "git://github.com/hahow/hahow-design.git",
  "keywords": [
    "react-component"
  ],
  "babel": {
    "plugins": [
      [
        "import",
        {
          "libraryName": "antd",
          "libraryDirectory": "es",
          "style": "css"
        }
      ],
      "inline-react-svg"
    ]
  }
}
